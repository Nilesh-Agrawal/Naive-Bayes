  <name>Assignment1-DS222</name>
  <version>0.0.1-SNAPSHOT</version>
  <description>Assignment1-DS222 MLLD</description>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>2.3</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
